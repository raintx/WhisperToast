package-as: WhisperToast

manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown

ignore:
  - .github
  - .gitignore
  - README.md
  - .release
  - .git
  - .gitattributes

enable-nolib-creation: no

move-folders:
  WhisperToast/Libs: Libs

localization:
